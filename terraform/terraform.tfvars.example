# Terraform Variables Configuration
# Copy this file to terraform.tfvars and fill in your actual values
# DO NOT commit terraform.tfvars to git!

project_id  = "knytt-backend"
region      = "us-central1"
github_repo = "your-github-username/knytt"

# Copy values from .env.cloudrun
database_url          = "postgresql://postgres.xxx:password@aws-1-us-east-2.pooler.supabase.com:6543/postgres"
supabase_url          = "https://xxx.supabase.co"
supabase_anon_key     = "eyJhbGc..."
supabase_service_key  = "eyJhbGc..."
jwt_secret_key        = "your-jwt-secret"
cors_origins          = "[\"http://localhost:3000\",\"https://knytt-frontend.pages.dev\",\"https://app.knytt.xyz\"]"
